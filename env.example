# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# App Configuration
VITE_APP_NAME=AWIGN Issue Compass
VITE_APP_VERSION=1.0.0

# WhatsApp API Configuration
# Add these environment variables in Netlify Dashboard: Site Settings > Environment Variables
WHATSAPP_API_KEY=your_whatsapp_api_key
WHATSAPP_NAMESPACE=your_whatsapp_namespace
WHATSAPP_TEMPLATE_NAME=your_whatsapp_template_name

# SMS API Configuration (Telspiel via AWIGN Core API)
# Add these environment variables in Netlify Dashboard: Site Settings > Environment Variables
VITE_SMS_ACCESS_TOKEN=your_sms_access_token
VITE_SMS_CLIENT=your_sms_client
VITE_SMS_UID=your_sms_uid
VITE_SMS_CLIENT_ID=core
VITE_SMS_SENDER_ID=IAWIGN
VITE_SMS_CHANNEL=telspiel
VITE_SMS_TICKET_CREATION_TEMPLATE_ID=your_ticket_creation_template_id
VITE_SMS_TICKET_UPDATE_TEMPLATE_ID=your_ticket_update_template_id

# Auto-Resolution Configuration
# Secret token for the auto-resolve function (generate a secure random string)
AUTO_RESOLVE_SECRET=your_secure_random_token_here

# Super Admin Configuration
# Used as performer for system-driven resolver/approver assignments
VITE_SUPER_ADMIN_ID=your_super_admin_user_id

# WhatsApp Template Parameters:
# {{1}} - Name of the ticket raiser
# {{2}} - Ticket number
# {{3}} - Ticket tracking link 